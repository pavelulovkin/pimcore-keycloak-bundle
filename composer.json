{
  "name": "iperson1337/pimcore-keycloak-bundle",
  "description": "Keycloak SSO Integration Bundle for Pimcore",
  "type": "pimcore-bundle",
  "license": "MIT",
  "require": {
    "php": "^8.1",
    "pimcore/pimcore": "^11.0",
    "symfony/framework-bundle": "^6.2",
    "knpuniversity/oauth2-client-bundle": "^2.10",
    "stevenmaguire/oauth2-keycloak": "^2.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5",
    "symfony/browser-kit": "^6.2",
    "symfony/phpunit-bridge": "^6.2"
  },
  "autoload": {
    "psr-4": {
      "Iperson1337\\PimcoreKeycloakBundle\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Iperson1337\\PimcoreKeycloakBundle\\Tests\\": "tests/"
    }
  },
  "extra": {
    "pimcore": {
      "bundles": [
        "PimcoreKeycloakBundle"
      ]
    }
  }
}
